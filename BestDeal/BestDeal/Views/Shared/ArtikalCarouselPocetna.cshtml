@model Artikal
        <div class="carousel-item">
            <div class="col-12">
                <div class="card mb-2">
                    <img class="card-img-top" src=@Model.URLSlike1
                         alt="Card image cap">
                    <div class="card-body">
                        <h4 class="card-title font-weight-bold">@Model.NazivArtikla</h4>
                        <p class="card-text">
                            @Model.KratkiOpis
                        </p>
                        <a class="btn btn-primary btn-md btn-rounded">Kupi!</a>
                    </div>
                </div>
            </div>
        </div>
       
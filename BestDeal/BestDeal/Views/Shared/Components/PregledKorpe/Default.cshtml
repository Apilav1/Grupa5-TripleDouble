@model KorpaViewModel

@if (Model.korpa.ArtikliKolicina.Count > 0)
{

    <li>
        <a asp-controller="ShoppingCart">
            <span class="glyphicon glyphicon-shopping-cart"></span>
            <span id="cart-status">
                @Model.korpa.ArtikliKolicina.Count
            </span>
        </a>
    </li>
}
